{
  "name": "web-typescript-template",
  "version": "1.0.0",
  "description": "A template for a frontend with HTML and plain typescript",
  "private": true,
  "scripts": {
    "clean": "node 'scripts/clean.js'",
    "build": "npm run clean && npx tsc && npx webpack --config 'webpack.config.js'",
    "tsc": "npx tsc --watch",
    "pack": "npx webpack --watch --config 'webpack.config.js'",
    "serve": "npx ws -d 'web-frontend'",
    "dev": "npx concurrently \"npm:tsc\" \"npm:pack\" \"npm:serve\"",
    "test": "echo 'test'"
  },
  "keywords": [
    "web",
    "html",
    "template",
    "plain",
    "typescript",
    "webpack"
  ],
  "author": "Tino Steinort",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "concurrently": "5.0.0",
    "local-web-server": "3.0.7",
    "rimraf": "3.0.0",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  }
}
